  "dependencies": {
    "@import-map-package/styled-system": "^0.0.1"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.46.0",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "8.3.0",
    "typescript": "5.6.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
